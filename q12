#include <stdio.h>

int main() {
    int num, i;

    // Solicita ao usuário o número
    printf("Digite um número inteiro para ver a tabuada: ");
    scanf("%d", &num);

    // Exibe a tabuada do número
    printf("Tabuada do número %d:\n", num);

    // Laço para calcular e exibir a tabuada de 1 a 10
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", num, i, num * i);
    }

    return 0;
}
